<!DOCTYPE html>
<html>
<head>
  <title>MyCityMyTeam</title>


  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
  <!--Import Google Icon Font-->
  <link href="http://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet">
  <!--Import materialize.css-->
  <link type="text/css" rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/materialize/0.97.6/css/materialize.min.css"  media="screen,projection"/>

  <meta name="viewport" content="width=device-width, initial-scale=1.0"/>
</head>
<body>

  <nav>
    <div class="nav-wrapper blue-grey darken-2">
      <a href="/" class="brand-logo">MyCityMyTeam</a>
      <a href="#" data-activates="mobile-demo" class="button-collapse"><i class="material-icons">view_headline</i></a>
      <ul class="right hide-on-med-and-down">
        <% if current_user %>
        <li><a href="#app-info">What is MyCityMyTeam?</a></li>
        <li><a href="#sports-info">Sports</a></li>
        <li><a href="#cities">Cities</a></li>
        <li><%= link_to "Profile", new_user_path %></li>
        <li><%= link_to "Log Out", logout_path %></li>
      </ul>
      <ul class="side-nav" id="mobile-demo">
        <li><a href="#app-info">What is MyCityMyTeam?</a></li>
        <li><a href="#sports-info">Sports</a></li>
        <li><a href="#cities">Cities</a></li>
        <li><%= link_to "Profile", new_user_path %></li>
        <li><%= link_to "Log Out", logout_path %></li>
      </ul>
      <% end %>
    </div>
  </nav>

  <!--Import jQuery before materialize.js-->
  <script type="text/javascript" src="https://code.jquery.com/jquery-2.1.1.min.js"></script>
  <script type="text/javascript" src="https://cdnjs.cloudflare.com/ajax/libs/materialize/0.97.6/js/materialize.min.js"></script>
  <script src="https://code.highcharts.com/highcharts.js"></script>


<%= yield %>

<footer>
  <div class="footer-copyright center">
    Â© 2016 Copyright Made By Scott Madrid
  </div>
</footer>

</body>
</html>
