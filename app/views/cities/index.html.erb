<% @cities.each do |c| %>
<div class="row-container">
  <div class="col s12 m6">
    <div class="card blue-grey darken-1">
      <div class="card-content white-text">
        <span class="card-title"><%= link_to "#{c.city_name}", city_path(c) %></span>
        <p>
          <strong>Date Founded:</strong> <%= c.date_founded %><br>
          <strong>Population:</strong> <%= c.population %><br>
          <strong>Nicknames:</strong> <%= c.nicknames %><br>
          <strong>Header URL:</strong> <%= c.header_pic %>
        </p>
      </div>
      <div class="card-action">
        <%= link_to "Edit", edit_city_path(c) %>
      </div>
    </div>
  </div>
</div>
<% end %>
